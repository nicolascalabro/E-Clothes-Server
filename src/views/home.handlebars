<div class="dashboard">
  <h1 class="dashboard-title">Home</h1>
  
  <div class="product-grid">
    {{#each products}}
      <div class="product-card">
        <h2 class="product-title">Title: {{this.title}}</h2>
        <h2 class="product-title">Description: {{this.description}}</h2>
        <h3 class="product-price">Price: ${{this.price}}</h3>
      </div>
    {{/each}}
  </div>

</div>